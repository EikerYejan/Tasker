{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "module": "ES2020",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": true,
    "useUnknownInCatchVariables": false
  }
}
