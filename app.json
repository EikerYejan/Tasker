{
  "expo": {
    "name": "TasksZen",
    "slug": "taskszen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "plugins": ["@react-native-google-signin/google-signin"],
    "splash": {
      "image": "./assets/tasker_splash_light.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eikeryejan.taskzen",
      "userInterfaceStyle": "automatic",
      "config": {"usesNonExemptEncryption": false},
      "splash": {
        "image": "./assets/tasker_splash_light.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/tasker_splash_dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tasker_splash_light.png",
        "backgroundColor": "#ffffff",
        "userInterfaceStyle": "automatic"
      },
      "splash": {
        "image": "./assets/tasker_splash_light.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/tasker_splash_dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      },
      "package": "com.eikeryejan.taskzen",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    }
  }
}
